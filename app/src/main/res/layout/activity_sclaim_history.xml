<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/sclaim_history_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D6D0C8"
    tools:context=".Students.SClaimHistoryActivity">

    <TextView
        android:id="@+id/s_ch_txt_itemname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Item Name"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/s_ch_txt_status"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.088"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/s_ch_txt_claimeditems"
        app:layout_constraintVertical_bias="0.47" />

    <TextView
        android:id="@+id/s_ch_txt_datefound"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Date Found"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/s_ch_txt_status"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.09"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/s_ch_txt_itemname"
        app:layout_constraintVertical_bias="0.516" />

    <TextView
        android:id="@+id/s_ch_txt_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Status"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/s_ch_txt_actualstatus"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.072"
        app:layout_constraintStart_toEndOf="@+id/imageView5" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="164dp"
        android:layout_height="147dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.214"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.225"
        app:srcCompat="@drawable/profile" />

    <TextView
        android:id="@+id/s_ch_txt_actualitemname"
        android:layout_width="74dp"
        android:layout_height="15dp"
        android:text="Umbrellla"

        app:layout_constraintBottom_toTopOf="@+id/s_ch_txt_datefound"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.091"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/s_ch_txt_itemname"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/s_ch_txt_actualdatefound"
        android:layout_width="74dp"
        android:layout_height="15dp"
        android:text="10/21/24"

        app:layout_constraintBottom_toTopOf="@+id/s_ch_txt_status"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.091"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/s_ch_txt_datefound"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/s_ch_txt_actualstatus"
        android:layout_width="73dp"
        android:layout_height="23dp"
        android:layout_marginBottom="4dp"
        android:text="Claimed"
        android:textColor="#FF9800"
        app:layout_constraintBottom_toBottomOf="@+id/imageView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.099"
        app:layout_constraintStart_toEndOf="@+id/imageView5" />

    <TextView
        android:id="@+id/s_ch_txt_claimeditems"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Your Claimed Items"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/sclaim_history_home_btn"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:backgroundTint="#ADA192"
        android:text="Home"
        android:onClick="Home"
        android:textSize="17sp"
        android:layout_marginTop="825dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.499" />

</androidx.constraintlayout.widget.ConstraintLayout>